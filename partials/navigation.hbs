<select type="select" class="nav-small">
  {{#foreach navigation}}
    <option value="{{slug}}">{{label}}</option>
  {{/foreach}}
</select>
<ul class="nav-big">
    {{#foreach navigation}}
    <li class="nav-{{slug}}{{#if current}} nav-current{{/if}}" role="presentation"><a href="{{url absolute="true"}}">{{label}}</a></li>
    {{/foreach}}
</ul>
